#include <iostream>

#include <ctime>

#include <random>

#include <fstream>

using namespace std;

// define N as 10

#define N 10

// getRdnum function

int getRdnum()

{

    // return random number between 1 and 50

    return 1 + rand() % 50;

}



int isPrime(int n)

{

    // loop from 2 to n/2

    for (int i = 2; i < n / 2; i++)

    {

        // if factor found

        if (n % i == 0)

            return 0;

    }

    return 1;

}

